<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Superadmin.Activity.superadmin_vista_supervisor2">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="411dp"
            android:layout_height="80dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="1dp"
            android:layout_marginBottom="1dp"
            android:background="@drawable/recto_background"
            android:orientation="horizontal"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <LinearLayout
                android:layout_width="311dp"
                android:layout_height="match_parent"
                android:orientation="horizontal"></LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/imageButton6"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#00FFFFFF"
                    android:contentDescription="hhh"
                    app:srcCompat="@drawable/image_profile_white" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="111dp"
            android:layout_gravity="center_vertical"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="36dp"
                android:orientation="horizontal"></LinearLayout>

            <LinearLayout
                android:layout_width="407dp"
                android:layout_height="56dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="10dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"></LinearLayout>


                    <LinearLayout
                        android:layout_width="352dp"
                        android:layout_height="56dp"
                        android:layout_gravity="center"
                        android:background="@drawable/list_morado_border_superadmin"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/textView53"
                            android:layout_width="222dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:background="@null"
                            android:hint="Buscar..."
                            android:inputType="text"
                            android:padding="10dp"
                            android:singleLine="true"
                            android:textSize="20sp" />

                        <ImageView
                            android:id="@+id/imageView17"
                            android:layout_width="72dp"
                            android:layout_height="46dp"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:src="@drawable/baseline_search_24" />

                    </LinearLayout>





            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="27dp"
            android:orientation="horizontal" />

        <ScrollView
            android:layout_width="381dp"
            android:layout_height="448dp"
            android:layout_gravity="center">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recycler_view_supervisor124"
                    android:layout_width="390dp"
                    android:layout_height="match_parent" />
            </LinearLayout>
        </ScrollView>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal"></LinearLayout>

        <LinearLayout
            android:layout_width="393dp"
            android:layout_height="84dp"
            android:layout_marginStart="1dp"
            android:layout_marginEnd="1dp"
            android:layout_marginBottom="-5dp"
            android:background="@drawable/recto_background"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout7"
            app:layout_constraintVertical_bias="0.92">

            <LinearLayout
                android:id="@+id/linearLayoutHome_SuperAdmin"
                android:layout_width="6dp"
                android:layout_height="70dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <ImageButton
                    android:id="@+id/buttonhomesuper"
                    android:layout_width="wrap_content"
                    android:layout_height="46dp"
                    android:background="@null"
                    android:scaleType="fitCenter"
                    android:scaleX="0.9"
                    android:scaleY="0.9"
                    android:src="@drawable/addresspic" />

                <TextView
                    android:id="@+id/textViewSitio_admin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/sitio_admin"
                    android:textColor="@color/white"
                    android:textStyle="bold"
                    android:visibility="gone" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/layoutHistorial_superadmin"
                android:layout_width="4dp"
                android:layout_height="70dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <ImageButton
                    android:id="@+id/buttonhistorialsuper"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:background="@null"
                    android:scaleType="fitCenter"
                    android:scaleX="0.9"
                    android:scaleY="0.9"
                    android:src="@drawable/historialpic" />

                <TextView
                    android:id="@+id/textViewUsuario_Admin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/usuario_admin"
                    android:textColor="@color/white"
                    android:textStyle="bold"
                    android:visibility="gone" />

            </LinearLayout>
        </LinearLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>