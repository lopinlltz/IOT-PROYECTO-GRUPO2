<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="16dp"
    tools:context=".Supervisor.Activity.EquipmentDetailsActivity">

    <TextView
        android:id="@+id/textViewSKU"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="SKU"
        android:textSize="16sp"
        android:layout_marginTop="8dp" />

    <TextView
        android:id="@+id/textViewNumeroSerie"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Número de serie"
        android:textSize="16sp"
        android:layout_marginTop="8dp" />

    <TextView
        android:id="@+id/textViewMarca"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Marca"
        android:textSize="16sp"
        android:layout_marginTop="8dp" />

    <TextView
        android:id="@+id/textViewModelo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Modelo"
        android:textSize="16sp"
        android:layout_marginTop="8dp" />

    <TextView
        android:id="@+id/textViewDescripcion"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Descripción"
        android:textSize="16sp"
        android:layout_marginTop="8dp" />

    <TextView
        android:id="@+id/textViewFechaRegistro"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Fecha de registro"
        android:textSize="16sp"
        android:layout_marginTop="8dp" />

</LinearLayout>
